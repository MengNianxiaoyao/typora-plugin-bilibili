{
  "name": "typora-plugin-bilibili",
  "version": "2.0.0",
  "description": "Typora粘贴图片自动上传到Bilibili",
  "main": "index.js",
  "scripts": {
    "pkg": "pkg . --out-path=dist/"
  },
  "bin": "./index.js",
  "targets": [
    "node14-win-x64",
    "node14-macos-x64"
  ],
  "pkg": {
    "targets": [
      "node14-win-x64",
      "node14-macos-x64"
    ],
    "assets": [
      "./node_modules/**/*"
    ]
  },
  "keywords": [
    "Typora",
    "Bilibili",
    "Typora-plugin，img-upload"
  ],
  "author": "xlzy520",
  "license": "ISC",
  "dependencies": {
    "form-data": "^4.0.0"
  }
}
